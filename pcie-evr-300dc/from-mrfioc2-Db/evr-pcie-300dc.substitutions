file "evrbase.db"
{pattern
{P, OBJ, EVNT1HZ}
{"$(SYS)", "$(DEVICE)", "$(EVTHZ)"}
}

file "databuftxCtrl.db"
{pattern
{P, OBJ}
{"$(SYS)", "$(DEVICE):BUFTX"}
}


file "evrmap.db"
{pattern
{NAME, OBJ, func, EVT}
{"$(SYS)Evt:Blink0-SP", "$(DEVICE)", Blink, 15}
{"$(SYS)Evt:Blink1-SP", "$(DEVICE)", Blink, 0}
{"$(SYS)Evt:ResetPS-SP","$(DEVICE)", "Reset PS", 123}
}
